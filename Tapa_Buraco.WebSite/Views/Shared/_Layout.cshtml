    <link href="~/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/font-awesome/css/font-awesome.css" rel="stylesheet">

    <link href="~/css/animate.css" rel="stylesheet">
    <link href="~/css/style.css" rel="stylesheet">

    <!-- Bootstrap Dropdown Hover CSS -->
    @*<link href="~/css/bootstrap-dropdownhover.min.css" rel="stylesheet">*@

    <!-- Bootstrap Dropdown Hover JS -->
    @*<script src="~/js/bootstrap-dropdownhover.min.js"></script>*@


</head>

<body class="top-navigation pace-running fixed-sidebar mini-navbar boxed-layout" id="divBody" ng-app="DashboardApp">
    <div id="url_Base_Site" data-request-url='@(string.Format("{0}://{1}{2}", Request.Url.Scheme, Request.Url.Authority.Replace("/", ""), (!string.IsNullOrEmpty(Url.Content("~").Replace("/", "")) ? ("/" + Url.Content("~").Replace("/", "")) : "")))'></div>
    <style>
        .navbar-brand {
            padding: 20px !important;
        }

        .imageOn {
            opacity: 1;
        }

        .imageOff {
            opacity: 0.5;
        }

        .fixed-top {
            position: fixed;
            top: 0;
            right: 0;
            left: 0;
            z-index: 1030;
        }

    </style>
    <div id="divAlert"></div>
    <div class="clearfix" style="width:100%;height:30px;"></div>

    <div id="wrapper">
        <div id="page-wrapper" class="gray-bg" style="min-height: 607px;">
            <div class="row border-bottom white-bg">
                <nav class="navbar navbar-static-top" role="navigation">
                    <!--navbar-fixed-top-->
                    @Html.Partial("_Menu")
                </nav>
            </div>
            
            <div class="wrapper wrapper-content">
                <div class="container">

                    @RenderBody()

                </div>
            </div>
            <div class="footer">
                <!-- fixed -->
                <div class="pull-right">
                    <strong>TAPA BURACO</strong>
                </div>
                <div>
                    <strong>Copyright</strong> TAPA BURACO © 2024
                </div>
            </div>

        </div>
    </div>

    <div class="clearfix" style="width:100%;height:30px;"></div>

    @Html.Partial("_Waiting")

    <script src="~/Scripts/site/Layout.js"></script>

    <script>
        $("#spanWaitingFull").text("Aguarde...");
        $(document).ajaxStart(
            function () {
                $.blockUI({ baseZ: 99999, message: $("#waitingFull").html(), css: { border: 'none', padding: '15px', backgroundColor: '#000', '-webkit-border-radius': '10px', '-moz-border-radius': '10px', opacity: .5, color: '#fff' } })
            }).ajaxStop(
                function () {
                    $.unblockUI()
                });
    </script>
</body>
</html>