    <script src="~/Scripts/site/Login.js"></script>
</head>

<body class="gray-bg">
    <div id="url_Base_Site" data-request-url='@(string.Format("{0}://{1}{2}", Request.Url.Scheme, Request.Url.Authority.Replace("/", ""), (!string.IsNullOrEmpty(Url.Content("~").Replace("/", "")) ? ("/" + Url.Content("~").Replace("/", "")) : "")))'></div>

    <div id="divAlert"></div>
    <div class="clearfix" style="width:100%;height:30px;"></div>

    <div class="middle-box text-center loginscreen  animated fadeInDown">
        <div>
            <div>
                <img src="~/Content/Images/img.png" style="width: 300px; margin-bottom:10px;" />
            </div>

            <form class="box tile animated active form-signin" id="box-login">
                <div class="form-group">
                    <input type="text" class="form-control m-b-10" placeholder="Login" id="textBoxLogin" required="required">
                </div>
                <div class="form-group">
                    <input type="password" class="form-control" placeholder="Senha" id="textBoxPassword">
                </div>

                <div class="m-t-20">
                    <button type="button" id="buttonLogIn" class="btn btn-primary block full-width m-b">Entrar</button>
                </div>
            </form>
        </div>
    </div>
</body>
