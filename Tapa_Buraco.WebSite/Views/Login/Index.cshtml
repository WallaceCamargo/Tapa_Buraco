    <!-- JTable JS -->
    <script src="~/Scripts/jtable/jquery.jtable.js"></script>
    <script src="~/Scripts/jtable/jquery.jtable.bootstrap.theme.js"></script>
    <script src="~/Content/themes/ecoporto/jquery-ui.js"></script>
    <link href="~/Content/themes/ecoporto/jquery-ui.css" rel="stylesheet" />
